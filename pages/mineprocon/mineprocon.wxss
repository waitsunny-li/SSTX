.proscroll {
  width: 100vw;
  height: 100vh;
}
page {
  background-color: #f5f5f5;
}
.tab-class {
  background-color: var(--assColor) !important;
  border-radius: 80rpx 80rpx 0 0 !important;
}
.tab-reset {
  background-color: var(--assColor) !important;
  border-radius: 0 0 0 0 !important;
}
.positiontab {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
}
.top-position {
  width: 100vw;
  background-color: #ffffff;
}
.top-position .location {
  width: 100vw;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.top-position .location .province-city {
  width: 460rpx !important;
  padding: 10rpx;
}
.top-position .location .cate-wrap {
  width: 400rpx;
  display: flex;
  align-items: center;
}
.top-position .location .cate-wrap .cate-name {
  font-size: 30rpx;
  color: #000000;
}
.top-position .location .cate-wrap .picker {
  width: 200rpx;
  height: 60rpx;
  color: var(--assColor);
  font-size: 28rpx;
  background-color: #f7f7f7;
  display: flex;
  align-items: center;
  border-radius: 10rpx;
  justify-content: center;
}
.top-position .location .cate-wrap .picker .picker-text {
  width: 180rpx;
  font-size: 28rpx;
}
.project-wrap {
  width: 100vw;
}
.project-wrap .procon-swiper {
  width: 100%;
  height: calc(100vh - 80rpx);
}
.project-wrap .procon-swiper .procon-swiper-time {
  width: 100%;
  height: 100%;
}
.project-wrap .procon-swiper .procon-swiper-time .proscroll {
  width: 100%;
  height: 100%;
}
.no-data {
  position: fixed;
  width: 100vw;
  height: 200rpx;
  top: 50%;
  transform: translateY(-50%);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.no-data .iconfont {
  font-size: 90rpx;
  color: var(--assColor);
}
.no-data .no-text {
  margin-top: 20rpx;
  color: #999999;
  font-size: 30rpx;
}
