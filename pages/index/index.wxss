.top-logo {
  width: 100vw;
  display: flex;
  margin-bottom: 8rpx;
}
.top-logo .title-text {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  color: #c10202;
  font-weight: 700;
}
.top-logo image {
  width: 100rpx;
  height: 100rpx;
}
.index-swiper {
  width: 100vw;
  height: 360rpx;
}
.index-swiper .index-swiper-item {
  width: 100%;
  height: 100%;
}
.index-swiper .index-swiper-item .index-swiper-navigator {
  width: 100%;
  height: 100%;
}
.index-swiper .index-swiper-item .index-swiper-navigator image {
  width: 100%;
  height: 100%;
}
.publish-support {
  width: 100vw;
  padding: 30rpx 0;
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin-bottom: 30rpx;
}
.publish-support .item-wrap {
  width: 260rpx;
  height: 140rpx;
  padding: 10rpx;
  box-sizing: border-box;
  background-color: #cd2b29;
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.publish-support .item-wrap .iconfont {
  color: #ffffff;
  font-size: 50rpx;
}
.publish-support .item-wrap .title {
  color: #ffffff;
  font-size: 24rpx;
}
.publish-support .item-wrap .desc {
  color: #ffffff;
  font-size: 22rpx;
}
.register-number {
  width: 30%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}
.register-number .number-wrap {
  width: 100%;
  display: flex;
  font-size: 26rpx;
}
.register-number .number-wrap .number {
  color: #e1211f;
  font-size: 25rpx;
}
.title-tab-wrap {
  width: 100vw;
  height: 110rpx;
  padding: 0 20rpx 0 0;
  border-bottom: 1px solid #d9d9d9;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}
.title-tab-wrap .project-connection {
  flex: 1;
  margin-left: 20rpx;
  padding: 10rpx 10rpx;
  border: 1px solid #d9d9d9;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  border-radius: 8rpx 8rpx 0 0;
}
.title-tab-wrap .project-connection .item {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #bbbbbb;
}
.title-tab-wrap .project-connection .item .iconfont {
  color: #434343;
  font-size: 35rpx;
}
.title-tab-wrap .project-connection .item .text {
  color: #434343;
  font-size: 24rpx;
}
