.location-wrap {
  width: 100%;
  height: 75rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.location-wrap .left-location {
  display: flex;
  align-items: center;
}
.location-wrap .left-location .head-title {
  width: 120rpx;
  font-size: 28rpx;
  margin-left: 5rpx;
}
.location-wrap .left-location .select-wrap {
  width: 100%;
  margin-left: 10rpx;
}
.location-wrap .left-location .select-wrap .picker-wrap {
  width: 100%;
  overflow: hidden;
}
.location-wrap .left-location .select-wrap .picker-wrap .picker {
  width: 140rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin-right: 10rpx;
  padding: 0 10rpx;
  box-sizing: border-box;
  border-radius: 5rpx;
  font-size: 28rpx;
  float: left;
  background-color: #f7f7f7;
  color: #666666;
  display: flex;
  justify-content: space-around;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
