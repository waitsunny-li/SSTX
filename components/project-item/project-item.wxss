.project-item {
  width: 100vw;
  height: 220rpx;
  padding: 20rpx;
  box-sizing: border-box;
  border-bottom: 1px dashed #e8e8e8;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.project-item .left-item {
  width: 130rpx;
  height: 130rpx;
  position: relative;
}
.project-item .left-item .top-icon {
  width: 100%;
  height: 35rpx;
  top: -35rpx;
  position: absolute;
  background-color: #efbf00;
  border-radius: 8rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.project-item .left-item .top-icon .text {
  color: #000000;
  font-size: 25rpx;
  font-weight: 700;
  margin-right: 25rpx;
}
.project-item .left-item .top-icon .iconfont {
  position: absolute;
  top: -15rpx;
  right: 10rpx;
  font-size: 48rpx;
  color: var(--themeColor);
}
.project-item .left-item .default-img {
  width: 100%;
  height: 100%;
  background-color: red;
  border-radius: 8rpx;
}
.project-item .left-item .default-img image {
  width: 100%;
  height: 100%;
}
.project-item .right-item {
  flex: 1;
  height: 100%;
  padding: 20rpx;
  box-sizing: border-box;
}
.project-item .right-item .title-section {
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.project-item .right-item .title-section .title {
  width: 60%;
  color: #000000;
  font-size: 32rpx;
}
.project-item .right-item .title-section .time {
  width: 40%;
  color: #a4a4a4;
  font-size: 24rpx;
}
