.project-item {
  position: relative;
  width: 100vw;
  height: 230rpx;
  padding: 20rpx 20rpx 10rpx 20rpx;
  box-sizing: border-box;
  border-bottom: 1px dashed #e8e8e8;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #ffffff;
}
.project-item .left-item {
  width: 130rpx;
  height: 130rpx;
  position: relative;
}
.project-item .left-item .top-icon {
  width: 100%;
  height: 35rpx;
  top: -35rpx;
  position: absolute;
  background-color: #efbf00;
  border-radius: 8rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.project-item .left-item .top-icon .text {
  color: #000000;
  font-size: 25rpx;
  font-weight: 700;
  margin-right: 25rpx;
}
.project-item .left-item .top-icon .iconfont {
  position: absolute;
  top: -15rpx;
  right: 10rpx;
  font-size: 45rpx;
  color: var(--themeColor);
}
.project-item .left-item .default-img {
  width: 100%;
  height: 100%;
  background-color: red;
  border-radius: 8rpx;
}
.project-item .left-item .default-img image {
  width: 100%;
  height: 100%;
}
.project-item .right-item {
  width: 575rpx;
  height: 130rpx;
  padding: 0 0 0 20rpx;
  box-sizing: border-box;
}
.project-item .right-item .title-section {
  width: 100%;
  display: flex;
  align-items: center;
}
.project-item .right-item .title-section .title {
  width: 60%;
  color: #000000;
  font-size: 32rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-left: 8rpx;
}
.project-item .right-item .title-section .title-1 {
  color: var(--themeColor);
  font-size: 20rpx;
  font-weight: 600;
}
.project-item .right-item .desc {
  width: 100%;
  margin-top: 5rpx;
  text-indent: 40rpx;
  color: #525151;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.project-item .right-item .desc .index {
  display: inline;
  font-size: 24rpx;
  color: var(--themeColor);
}
.project-item .right-item .bottom-section {
  width: 100%;
  margin-top: 15rpx;
  display: flex;
  flex-direction: row-reverse;
  align-items: center;
}
.project-item .right-item .bottom-section .detail {
  width: 100rpx;
  border: 1px solid #e8e8e8;
  color: #a4a4a4;
  font-size: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 5rpx;
}
.project-item .star-position {
  position: absolute;
  right: 20rpx;
  top: 10rpx;
}
