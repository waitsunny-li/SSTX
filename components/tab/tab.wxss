.tab-wrap {
  width: 100vw;
  margin-top: 20rpx;
  display: flex;
  justify-content: space-around;
  border: 1px solid #bbbbbb;
  background-color: #ffffff;
}
.tab-wrap .tab-item {
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  transition: 0.3s;
}
.tab-wrap .tab-item .iconfont {
  font-size: 38rpx;
  color: #ee8300;
  font-weight: 700;
}
.tab-wrap .tab-item .tab-text {
  color: #000000;
  font-size: 28rpx;
}
.tab-wrap .tab-item .point {
  color: #ee8300;
}
.tab-wrap .active {
  background-color: #970d0d;
}
.tab-wrap .active .tab-text {
  color: #ffffff !important;
}
.tab-wrap .tab-item:nth-child(2n+1) {
  border-left: 1px solid #bbb;
  border-right: 1px solid #bbb;
}
